<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>StableServlet</servlet-name>
    <display-name>StableServlet</display-name>
    <description></description>
    <servlet-class>handle.servlet.StableServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StableServlet</servlet-name>
    <url-pattern>/stable</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DonkeySiblingsServlet</servlet-name>
    <display-name>DonkeySiblingsServlet</display-name>
    <description></description>
    <servlet-class>handle.servlet.DonkeySiblingsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DonkeySiblingsServlet</servlet-name>
    <url-pattern>/donkeysiblings</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DonkeyServlet</servlet-name>
    <display-name>DonkeyServlet</display-name>
    <description></description>
    <servlet-class>handle.servlet.DonkeyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DonkeyServlet</servlet-name>
    <url-pattern>/donkey</url-pattern>
  </servlet-mapping>  
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>

</web-app>